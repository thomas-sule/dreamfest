{
  "name": "dreamfest",
  "type": "module",
  "version": "0.1.0",
  "description": "NZ's premier culture festival",
  "main": "index.js",
  "scripts": {
    "start": "node dist/server.js",
    "dev": "run-p dev:client dev:server",
    "dev:client": "vite",
    "dev:server": "tsx watch server/index.ts",
    "build": "run-s build:client build:server",
    "build:client": "vite build",
    "build:server": "esbuild --packages=external --platform=node --format=esm --outfile=dist/server.js --bundle server/index.ts",
    "test": "vitest",
    "knex": "knex --knexfile ./server/db/knexfile.js",
    "lint": "eslint --ext .js,.jsx,.ts,.tsx .",
    "format": "prettier -w '**/*.{js,jsx,ts,tsx}'"
  },
  "author": "Dev Academy Aotearoa",
  "license": "ISC",
  "keywords": [
    "db",
    "database",
    "backend"
  ],
  "homepage": "https://github.com/dev-academy-challenges/dreamfest#readme",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/dev-academy-challenges/dreamfest.git"
  },
  "eslintConfig": {
    "extends": "@devacademy"
  },
  "dependencies": {
    "express": "^4.17.2",
    "knex": "^2.5.1",
    "sqlite3": "^5.1.6"
  },
  "devDependencies": {
    "@devacademy/eslint-config": "^1.10.1",
    "@tanstack/react-query": "^5.22.2",
    "@testing-library/dom": "^8.17.1",
    "@testing-library/jest-dom": "^6.2.0",
    "@testing-library/react": "^14.0.0",
    "@testing-library/user-event": "^14.5.1",
    "@types/express": "^4.17.14",
    "@types/superagent": "^8.1.1",
    "@types/supertest": "^6.0.2",
    "@vitejs/plugin-react": "^4.2.1",
    "esbuild": "^0.18.16",
    "eslint": "^8.56.0",
    "eslint-plugin-import": "^2.29.1",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-prettier": "^4.0.0",
    "eslint-plugin-promise": "^6.1.1",
    "jsdom": "^22.1.0",
    "nock": "^13.3.3",
    "nodemon": "^2.0.15",
    "npm-run-all": "^4.1.5",
    "prettier": "^3.2.5",
    "react": "^18.0.2",
    "react-dom": "^18.2.0",
    "react-router-dom": "^6.11.2",
    "superagent": "^8.1.2",
    "supertest": "^6.3.4",
    "tsx": "^3.12.7",
    "vite": "^5.0.0",
    "vitest": "^1.2.0"
  },
  "prettier": {
    "semi": false,
    "singleQuote": true
  }
}